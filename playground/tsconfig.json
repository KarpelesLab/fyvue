{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "exactOptionalPropertyTypes": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "vite/client",
      "../dist/dist/components",
      "../dist"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@karpeleslab/fyvue": [
        "../dist/fyvue.mjs"
      ],
      "@karpeleslab/fyvue/*": [
        "../dist"
      ],
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/main.ts",
    "../typings/env.d.ts",
    "../typings/components.d.ts",
    "../src/**/*.ts",
    "../src/**/*.d.ts",
    "../src/**/*.tsx",
    "../src/**/*.vue",
  ]
}
