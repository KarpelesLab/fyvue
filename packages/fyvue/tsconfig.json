{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "dist/types",
    "outDir": "dist",
    "baseUrl": "packages/fyvue/",
    "allowJs": false,
    "strict": true,
    "noUnusedLocals": false,
    "lib": ["esnext", "dom"],
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client", "@types/node"],
    "removeComments": true,
    "noImplicitAny": false,
    "paths": {
      //"@karpeleslab/fyvue/*": [
      //  "./*"
      //],
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/dts/*.d.ts",
    "../../node_modules/vue-router/dist/vue-router.d.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
