{
    "name": "playground",
    "version": "1.0.0",
    "scripts": {
        "dev:live": "vite --force --port 3000 ",
        "dev:bundle": "vite --force --port 3000 --config vite.config.bundle.ts",
        "build": "yarn run build:client && yarn run build:server:noExternal",
        "build:client": "vite build --ssrManifest --outDir dist/client --config vite.config.bundle.ts",
        "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server --config vite.config.bundle.ts",
        "build:server:noExternal": "vite build --config vite.config.noexternal.ts --ssr src/entry-server.ts --outDir dist/server",
        "typecheck": "vue-tsc --noEmit",
        "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore"
    },
    "dependencies": {
        "@karpeleslab/i18next-klb-backend": "^0.1.0",
        "@vuelidate/core": "^2.0.0",
        "@vuelidate/validators": "^2.0.0",
        "@vueuse/core": "^9.5.0",
        "sitemap": "^7.1.1",
        "vue": "^3.2.45",
        "vue-code-highlight": "^0.7.8",
        "vue-router": "^4.1.6",
        "xml-formatter": "^2.6.1"
    },
    "devDependencies": {
        "@babel/types": "^7.20.2",
        "@headlessui/vue": "^1.7.4",
        "@heroicons/vue": "^2.0.13",
        "@karpeleslab/klbfw": "^0.1.12",
        "@rushstack/eslint-patch": "^1.1.0",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@tailwindcss/typography": "^0.5.7",
        "@types/node": "^18.11.9",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/server-renderer": "3.2.45",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.5.0",
        "eslint-plugin-vue": "^9.8.0",
        "express": "^4.18.2",
        "i18next": "^22.0.4",
        "pinia": "^2.0.23",
        "postcss": "^8.4.18",
        "prettier": "^2.5.1",
        "sass": "^1.56.0",
        "tailwindcss": "^3.2.2",
        "typescript": "~4.9.3",
        "unplugin-vue": "^3.1.3",
        "vite": "^3.2.4",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.9"
    }
}
