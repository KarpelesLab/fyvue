{
  "name": "@karpeleslab/fyvue",
  "private": true,
  "description": "Vue lib for KLB systems",
  "packageManager": "pnpm@7.14.1",
  "scripts": {
    "build": "rm -rf dist/ && rollup -c --bundleConfigAsCjs",
    "build:css": "npx tailwindcss -i ./src/global.scss -o ./dist/fyvue.css",
    "lint": "eslint --ext .js,.vue,.ts src/",
    "play": "pnpm run -C ./playground dev:live",
    "play:bundle": "pnpm run -C ./playground dev:bundle",
    "doc": "pnpm run -C ./doc docs:build",
    "doc:dev": "pnpm run -C ./doc docs:dev",
    "lint:check": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarpelesLab/fyvue.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KarpelesLab/fyvue/issues"
  },
  "homepage": "https://github.com/KarpelesLab/fyvue#readme",
  "peerDependencies": {
    "@headlessui/vue": "^1.7.4",
    "@karpeleslab/klbfw": "^0.1.12",
    "@vue/server-renderer": "3.2.45",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vueuse/core": "^9.5.0",
    "@vueuse/head": "^0.9.8",
    "i18next": "^22.0.4",
    "pinia": "^2.0.23",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.4",
    "@heroicons/vue": "^2.0.13",
    "@karpeleslab/klbfw": "^0.1.12",
    "@nuxt/devalue": "^2.0.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "^18.11.9",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/server-renderer": "3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vueuse/core": "^9.5.0",
    "@vueuse/head": "^0.9.8",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "i18next": "^22.0.4",
    "mitt": "^3.0.0",
    "pinia": "^2.0.23",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^3.0.0",
